{% extends "base.djhtml" %}


{% block vlu %}
class="aktiv"
{% endblock vlu %}


{% block content %}

<div>
  <h2>Antwortb√∂gen</h2>
  <div class="blockwrapper">
  <table class="t">
    <thead>
      <tr>
        <th>Studiengang</th>
        <th>Tutor</th>
        <th>Semester</th>
        <th>Eingetippt</th>
        <th>... von</th>
        <th class="buttons">
          <a href="/eval/editbogen/{{ vl.id  }}" class="button">Neuen Bogen eintippen</a>
        </th>
      </tr>
    </thead>
    <tbody>
      {% for b in vl.antwortboegen.all() %}
      <tr>
        <td>{{ b.studiengang|none }}</td>
        <td>{{ b.tutor|none }}</td>
        <td>{{ b.semester|none }}</td>
        <td>{{ b.date_created|date }}</td>
        <td>{{ b.user.username }}</td>
        <td class="buttons">
          <a href="/eval/editbogen/{{ vl.id  }}/{{ b.id }}" class="button">Bogen Bearbeiten</a>
        </td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
  </div>
</div>

{% endblock content %}
